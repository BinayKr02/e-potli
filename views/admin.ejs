<body>
    
    <style>
        body{
            background-color: #212529;
        }
    </style>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <a class="navbar-brand" href="#" style="margin-right: 10px;"><strong>E-Potli</strong></a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
      
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav mr-auto">
            <li class="nav-item active">
              <a class="nav-link" href="/admin"  style="margin-right: 10px;">Admin Console Home <span class="sr-only">(current)</span></a>
            </li>
          
           
          </ul>
        
        </div>
    </nav>
 
      <br>
      <br>
      <h1 style="color:white">Withdrawl Requests</h1>
      <table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Wallet ID</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Amount</th>
      <th scope="col">Approved/Delete</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i =0;i< withdrawl_requests_false.length;i++){%>
    <tr>
      <th scope="row"><%=withdrawl_requests_false[i].from%></th>
      <td><%=withdrawl_requests_false[i].name%></td>
      <td><%=withdrawl_requests_false[i].email%></td>
      <td><%=withdrawl_requests_false[i].amount%></td>
      <td><a href="/approve/<%= withdrawl_requests_false[i]._id %>"><i class="fas fa-check-circle fa-3x"></i> </a>  <a href="/delete/<%=withdrawl_requests_false[i]._id%>"><i class="far fa-times-circle fa-3x"></i></a></td>
    </tr><%}%>
  </tbody>
</table>
<br>
<br>
<hr style="color:white; width:100%">
<br><br>
<h1 style="color:white">Approved Withdrawls</h1>
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Wallet ID</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Amount</th>
      <th scope="col">Cancel Withdrawl</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i =0;i< withdrawl_requests_true.length;i++){%>
    <tr>
      <th scope="row"><%=withdrawl_requests_true[i].from%></th>
      <td><%=withdrawl_requests_true[i].name%></td>
      <td><%=withdrawl_requests_true[i].email%></td>
      <td><%=withdrawl_requests_true[i].amount%></td>
      <td><a href="/reject/<%=withdrawl_requests_true[i]._id%>"><i class="far fa-times-circle fa-3x"></i></a></td>
    </tr><%}%>
  </tbody>
</table>



</body>
</html>


